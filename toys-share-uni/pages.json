{
	"easycom": {
		"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app",
				"app-plus": {
					"titleNView": false //禁用原生导航栏
				}
			}
		},
		{
			"path": "pages/offerSharing/offerSharing",	//报价分享
			"aliasPath":"/offerSharing",
			"style": {
				"navigationBarTitleText": "offerSharing",
				"app-plus": {
					"titleNView": false //禁用原生导航栏
				}
			}
		},
		{
			"path": "pages/offerSharing/offerDetail",	//报价详情
			"aliasPath":"/offerDetail/:pid/:id",
			"style": {
				"navigationBarTitleText": "offerDetail",
				"app-plus": {
					"titleNView": false //禁用原生导航栏
				}
			}
		},
		{
			"path": "pages/offerSharing/offerContact",	//报价联系方式
			"aliasPath":"/offerContact/:id",
			"style": {
				"navigationBarTitleText": "offerContact",
				"app-plus": {
					"titleNView": false //禁用原生导航栏
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
